<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath osa.ora.MainClass</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-clean-compile-package</actionName>
            <displayName>clean-compile-package</displayName>
            <goals>
                <goal>clean</goal>
                <goal>compile</goal>
                <goal>package</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-clean install compile package</actionName>
            <displayName>clean install compile package</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
                <goal>compile</goal>
                <goal>package</goal>
            </goals>
        </action>
    </actions>
